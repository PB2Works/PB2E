<?xml version="1.0" encoding="utf-8" ?>
<extension xmlns="http://ns.adobe.com/air/extension/3.1">
	
	<id>com.pb2works.PB2E</id>
	<name>Plazma Burst 2 Extended Extension</name>
	<description>Expands the capabilities of Plazma Burst 2</description>
	<versionNumber>1.0</versionNumber>
	<copyright>Â© 2023, 89NA (PB2Works team)</copyright>
	
	<!--<platforms> must provide the native library's name, as well as the name of the initializer
	and finalizer, otherwise you will get error #3500.-->
	<platforms>
		<platform name="Windows-x86">
			<applicationDeployment>
				<nativeLibrary>PB2E.dll</nativeLibrary>
				<initializer>extensionInitializer</initializer>	<!--Name must NOT contain an underscore-->
				<finalizer>extensionFinalizer</finalizer>		<!--Name must NOT contain an underscore-->
			</applicationDeployment>
		</platform>
	</platforms>
	
</extension>